{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "import/extensions": [0, { "js": "always" }],
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "max-len": ["error", {"code": 120}],
    "no-console": "off"
  }
}
